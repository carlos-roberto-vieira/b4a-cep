Sub formata( cnumero As String ) 
	
	lblCepText.Text = lblCepText.Text & cnumero
	
	'CEP 78901122    78.901-122  (7)
	Dim c As String
			
	c = lblCepText.Text.Replace( ".", "" ).Replace( "-", "" )
 			
	Try
		c = c.SubString2( 0,2 ) & "." & c.SubString2(2,5 ) & "-" & c.SubString2(5,8)
					
		lblCepText.Text = c
				
	Catch
		 
	End Try
			
End Sub