Sub Activity_Create(FirstTime As Boolean)
	
	Activity.LoadLayout("Layout")
	
	lblCepText.Text = ""
	
	If FirstTime Then 
		
		DisableStrictMode
		
		Wait For(SolicitarPermissoes) Complete (PermissaoTotal As Boolean)
		
		If PermissaoTotal = False Then
			MsgboxAsync( "AppCEP Precisa de permissões especificas para continuar ", "Ops !" )
			Activity.Finish
			ExitApplication
		End If
		
	End If
 
	
End Sub
 
Sub Activity_Pause (UserClosed As Boolean)
End Sub
  
Sub paPesquisar_Click
	
	'78000999
	
	If lblCepText.Text.trim.Length < 8 Then
		MsgboxAsync( "Cep tem que pelo menos 8 numeros", "ops" )
		Return		
	End If
	
	'json
	
	ProgressDialogShow( "Aguarde ..."  )
	
	Try
		
		' https://api.postmon.com.br/v1/cep/76901122
	 
		Dim url As String = "http://viacep.com.br/ws/" & lblCepText.Text.Replace( ".", "" ).Replace( "-", "" ) & "/json/"
		Dim j As HttpJob
		 	
		j.Initialize( "", Me )
		j.Download( url )
		Wait For (j) JobDone (j  As HttpJob )
		
		Sleep(100)
			
		If j.Success Then
				
			Dim res As String
			res = j.GetString
		 	
			Dim jsp As JSONParser
			Dim m As Map
			
			 
			jsp.Initialize(res)
			Dim obj As Object= jsp.NextObject
			
			If obj Is Map Then
				
				m  = obj
				
				Dim cep As String
				
				 cep = m.Get( "logradouro" ) & CRLF & _
					   m.Get( "complemento" ) & CRLF & _
					  "BAIRRO : " & m.Get( "bairro" ) & CRLF & _
					  "CIDADE : " & m.Get( "localidade" ) & CRLF & _
					  "UF : " &  m.Get( "uf" ) & CRLF & _
					  "IBGE : " & m.Get( "ibge" ) 
					   
				
				ProgressDialogHide
				   
				If cep.LastIndexOf( "null" )   > 0 Then 				
					MsgboxAsync( "CEP não encontrado !", "Desculpe!" )
				Else
				 
						
					Msgbox2Async(cep, "Achei !", "Abrir Mapa", "", "Continuar", Null, False)
					Wait For Msgbox_Result (Result As Int)
					If Result = DialogResponse.POSITIVE Then
					 
							
						Dim MapIntent As Intent
						MapIntent.Initialize(MapIntent.ACTION_VIEW,"geo:0,0?q='" & m.Get( "logradouro" )  & "','" & m.Get( "localidade" ) & "','" & m.Get( "uf" ) & "'"  )
						MapIntent.SetComponent("googlemaps")
	
						Try
							StartActivity(MapIntent)
						Catch
							MsgboxAsync( "GoogleMaps não encontrado !", "Ops !")
						End Try
					
					End If
				 
				End If
	 
			End If
		 
		 
		End If
		
 		
		j.Release
		
		ProgressDialogHide
		
		
	Catch
		
		ProgressDialogHide
		
		Log( LastException.Message )
		
		MsgboxAsync("Erro inexperado, tente novamente !", "Ops!" )
		
	End Try
	
	
End Sub

 

